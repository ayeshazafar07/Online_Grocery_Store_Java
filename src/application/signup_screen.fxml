               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="signup_page_phoneNo" prefHeight="30.0" prefWidth="50.0" promptText="Phone No">
               <VBox.margin>
                  <Insets left="250.0" right="250.0" top="15.0" />
               </VBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="signup_page_address" prefHeight="30.0" prefWidth="50.0" promptText="Address">
               <VBox.margin>
                  <Insets left="250.0" right="250.0" top="15.0" />
               </VBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Button fx:id="signup_page_signup" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="90.0" text="SignUp" textFill="#6c7994">
